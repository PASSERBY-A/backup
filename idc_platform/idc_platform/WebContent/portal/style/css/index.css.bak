.index_conerCon1 ul li{ width:210px; padding-left:25px;overflow:hidden;text-overflow:ellipsis; white-space:nowrap; background:url(../img/icon3.jpg) no-repeat left center; border-bottom:1px solid #ced0cf; line-height:20px;}
.index_conerCon1 ul li span{ color:#f79135; margin-right:15px;}
.index_conerCon1 ul li a{ color:#757474; font-size:11px;}
.more_wrap{ position:relative;}
.more{ background:url(../img/more.jpg) no-repeat right center; float:right; display:block; margin-right:10px; padding-right:15px; font-weight:bold; font-size:11px; color:#58abbb; margin-top:5px; position:absolute; top:-15px; right:10px;}

