.index_line4Rit table tr td{ color:#fff; background:url(../img/icon9.jpg) no-repeat left center; text-align:left; border-bottom:1px solid #e0ded9;}
.index_line4Rit table tr td a{ display:block; width:70px; font-size:11px; overflow:hidden;text-overflow:ellipsis; white-space:nowrap;  color:#fff; padding-left:10px;}
.index_line4Rit table tr td a:hover{ color:#fff;}
