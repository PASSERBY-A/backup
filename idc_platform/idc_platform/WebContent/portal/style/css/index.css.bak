@charset "utf-8";
/* CSS Document */
/*img, div { behavior: url(/portal/style/iepngfix/iepngfix.htc);ie6 }*/
/*reset  css*/
*{ margin:0px; padding:0px; border-width:0px;}
body{ font-family:"微软雅黑"; font-size:12px; line-height:18px; color:#000; background:url(../img/index_bodyBg.jpg) repeat-x left top;}
.clearfloat{ clear:both; font-size:0px; line-height:0px; height:0px; visibility:hidden;}
a{ font-size:12px; color:#656464;  border:none;  text-decoration:none;}
a:hover{ text-decoration:underline; color:#0d80a6;}
ul{ list-style:none;}
h2{ font-size:12px;}
/*reset  end*/
#posts-container			{ width:400px; border:1px solid #ccc; -webkit-border-radius:10px; -moz-border-radius:10px;background-color: #fff; }
.post						{ padding:5px 10px 5px 100px; min-height:65px; border-bottom:1px solid #ccc; background:url(../img/dwloadmore.png) 5px 5px no-repeat; cursor:pointer;  }
.post:hover					{ background-color:lightblue; }
a.post-title 				{ font-weight:bold; font-size:12px; text-decoration:none; }
a.post-title:hover			{ text-decoration:underline; color:#900; }
a.post-more					{ color:#900; }
p.post-content				{ font-size:12px; line-height:17px; padding-bottom:0; }
.activate					{ background:url(loadmorespinner.gif) 140px 9px no-repeat #eee; }
/*head  css*/
/*出发弹出层  css*/
.cover_divBig{ width:100%; height:1100px; background-color:transparent; font-weight:normal; background:url(../img/cover_bg.png) repeat-x left top; position:absolute; z-index:20; }
.cover_divSmall{ width:400px; height:1000px; padding:5px 10px; position:absolute; font-weight:normal; margin:50px 0px 0px 400px; z-index:25;}
.btn1{ background-color:#eee; color:#999; font-weight:bold; text-align:center; padding:10px 0; cursor:pointer; border: 1px solid #666;}
.btn1:hover{ color:#666; }
/*出发弹出层  end*/
.wrap{ width:990px; margin:0px auto;}
/*line1 css*/
.index_line1{ width:990px; height:80px; position:relative;margin:0px auto;}
.index_line1Con{ width:990px; background:url(../img/index_headBg.jpg) no-repeat center top; height:80px;}
.index_line1Rit{ padding:28px 10px 0px 0px;}
.index_line1Rit1{ float:right;}
.index_line1Span1{ background:url(../img/icon1.gif) no-repeat left top; padding-left:30px; color:#58abbb; display:block; float:left;}
.index_line1Span2{ background: url(../img/button1.jpg) no-repeat left top; color:#fff; width:30px; height:17px; display:block; float:left; margin-left:10px; padding:0px 0px 0px 5px; cursor:pointer;}
.index_line1Span3{ width:23px; height:23px; background:url(../img/icon16.jpg) no-repeat left center; cursor:pointer; display:block; float:left; margin-left:8px; position:relative; top:-3px;}
.index_line1Span4{ width:23px; height:23px; background:url(../img/icon17.jpg) no-repeat left center; cursor:pointer; display:block; float:left; margin-left:8px; position:relative; top:-3px;}
.index_line1Span5{ width:23px; height:23px; background:url(../img/icon18.jpg) no-repeat left center; cursor:pointer; display:block; float:left; margin-left:8px; position:relative; top:-3px;}
.index_line1Span6{ width:15px; height:15px; display:block; float:left; background:url(../img/icon21.gif) no-repeat left center; margin-right:5px;cursor: pointer;}
.index_line1Span7{ width:100px; height:15px; display:block; float:left; margin-right:5px;color:#58abbb; }
.index_line1Span8{ width:10px; height:15px; display:block; float:left; margin-right:5px;color:red; }
/*line1 end*/
/*index_nav  css*/
.index_nav{ width:990px; margin:0px auto;}
.index_nav li{float:left; cursor:pointer;}
.index_nav li a{  width:183px; height:34px; padding-top:15px; display:block; color:#fff; text-align:center;}
.index_nav li a:hover{ text-decoration:none; color:#fff;}
.index_nav1,.index_nav2,.index_nav3,.index_nav4,.index_nav5{ width:87px; height:34px; background:url(../img/nav_img1.png) no-repeat 17px center;}
.index_nav2{background:url(../img/nav_img2.png) no-repeat 14px center;}
.index_nav3{background:url(../img/nav_img3.png) no-repeat 14px center;}
.index_nav4{background:url(../img/nav_img4.png) no-repeat 14px center;}
.index_nav5{background: url(../img/nav_img5.png) no-repeat 14px center;}
.index_line2{position:relative; width:990px; height:35px; margin:0px auto;}
.index_SubNav{ margin-top:10px; }
.index_SubNav li{ float:left; width:65px; background:url(../img/icon2.jpg) no-repeat left center; text-align:left; padding-left:20px;}
.index_SubNav li a,.index_SubNav li a:hover{ color:#fff;}
.Sub1{ margin-left:20px; position:absolute;}
.Sub2{ margin-left:200px; position:absolute;}
.Sub3{ margin-left:380px; position:absolute;}
.Sub4{ margin-left:505px; position:absolute;}
.Sub5{ margin-left:550px; position:absolute;}
.index_search { position:absolute; width:142px; height:21px; float:right; margin-top:8px; background:url(../img/search_bg.jpg) no-repeat; background-color:transparent; margin-right:10px; top:128px; right:0px;}
.index_search input{ width:88px; margin: 2px 0 0 10px;}
.nav_liOn{ background:url(../img/li_blockOn.jpg) repeat-x;}
/*index_nav  end*/

/*head  end*/	

/*index_line3  css*/
.index_line3{ width:950px; height:286px; padding:15px 20px;}
.index_line3Lef{ width:652px; height:285px; float:left; padding: 0 12px;}
.index_line3Rit{ width:250px; height:284px; float:right; padding: 0 12px;}

.coner_top1,.coner_bottom1{ width:250px; height:14px; overflow:hidden;}
.coner_topLef1,.coner_btmLef1{ width:13px; height:14px; display:block; float:left; background:url(../img/coner_topLef1.jpg) no-repeat left top;}
.coner_topMid1,.coner_btmMid1{ width:224px; height:12px; background-color:#fff; border-top:2px solid #ced9d6; display:block; float:left; font-size:0px; }
.coner_topRit1,.coner_btmRit1{ width:13px; height:14px; display:block; float:left; background:url(../img/coner_topRit1.jpg) no-repeat right top;}
.coner_btmLef1{ background:url(../img/coner_bottomLef1.jpg) no-repeat left top;}
.coner_btmMid1{ border-bottom:2px solid #ced9d6; border-top:none;}
.coner_btmRit1{ background:url(../img/coner_bottomRit1.jpg) no-repeat right top;}
.index_conerCon1{ width:246px; height:256px; border:2px solid #ced9d6; border-bottom:none; border-top:none; background-color:#fff; overflow:hidden;}
.index_conerCon1 h2{ height:16px; background:url(../img/h2_1.jpg) no-repeat left center; color:#808080; font-size:14px; padding-left:20px; margin-left:10px;}
.index_con1{width:242px;margin: 0 auto;overflow-y:auto;overflow-x:hidden; background-color:#e7ebe6; height:125px; margin-top:5px;}
.index_con1 span{ overflow:hidden;text-overflow:ellipsis;white-space:nowrap;width:230px; margin:0px auto;text-align:center; color:#ec8e25; display:block; margin-top:3px;}
.index_con1 p{ width:210px; margin:0px auto; color:#757474; text-indent:2em; font-size:11px; border-bottom:1px solid #cedbd4; overflow:hidden; padding:0px 5px;}
