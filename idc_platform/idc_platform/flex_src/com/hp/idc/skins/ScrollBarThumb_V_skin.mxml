<?xml version="1.0" encoding="utf-8"?>
<s:Skin xmlns:fx="http://ns.adobe.com/mxml/2009" 
		xmlns:s="library://ns.adobe.com/flex/spark">
	<fx:Metadata>
		[HostComponent("spark.components.Button")]
	</fx:Metadata>
	<s:states>
		<s:State name="up" />
		<s:State name="over" />
		<s:State name="down" />
		<s:State name="disabled" />
	</s:states>
	<s:Rect left="0" width="15" minHeight="24" height="100%">
		<s:fill>
			<s:LinearGradient>
				<s:entries>
					<s:GradientEntry color="0xFFFFFF" ratio="0" alpha="0.85" alpha.over="0.5"/>
					<s:GradientEntry color="0xD8D8D8" ratio="1" alpha="0.85" alpha.over="0.5"/>
				</s:entries>
			</s:LinearGradient>
		</s:fill>
		<s:stroke>
			<s:SolidColorStroke color="#969EA8" />				
		</s:stroke>
	</s:Rect>
	<s:Rect top="1" left="1" right="1" bottom="1" includeIn="over">
		<s:stroke>
			<s:SolidColorStroke color="#FFFFFF" alpha="0.5"/>
		</s:stroke>
	</s:Rect>	

	<s:VGroup left="4" height="8" gap="1" verticalCenter="0">
		<s:Line yFrom="0" yTo="0" xFrom="0" xTo="7">
			<s:stroke>
				<s:SolidColorStroke color="#909293"/>
			</s:stroke>
		</s:Line>
		<s:Line yFrom="0" yTo="0" xFrom="0" xTo="7">
			<s:stroke>
				<s:SolidColorStroke color="#909293"/>
			</s:stroke>
		</s:Line>
		<s:Line yFrom="0" yTo="0" xFrom="0" xTo="7">
			<s:stroke>
				<s:SolidColorStroke color="#909293"/>
			</s:stroke>
		</s:Line>
		<s:Line yFrom="0" yTo="0" xFrom="0" xTo="7">
			<s:stroke>
				<s:SolidColorStroke color="#909293"/>
			</s:stroke>
		</s:Line>
	</s:VGroup>
</s:Skin>
