<?xml version="1.0" encoding="utf-8"?>
<s:Skin xmlns:fx="http://ns.adobe.com/mxml/2009" 
		xmlns:s="library://ns.adobe.com/flex/spark">
	<fx:Metadata>
		[HostComponent("spark.components.Button")]
	</fx:Metadata>
	<s:states>
		<s:State name="up" />
		<s:State name="over" />
		<s:State name="down" />
		<s:State name="disabled" />
	</s:states>

	<s:Rect left="0" bottom="0" top="0" width="15">
		<s:stroke>
			<s:SolidColorStroke color="#DEE4EA" weight="1"/>
		</s:stroke>
	</s:Rect>
	<s:Rect left="1" bottom="0" top="0" width="13">
		<s:fill>
			<s:SolidColor alpha="1" color="#FFFFFF"/>
		</s:fill>
	</s:Rect>
	<s:Line top="1" left="1" bottom="1">
		<s:stroke>     
			<!--- @private -->
			<s:SolidColorStroke color="#dee3e6" weight="1" />
		</s:stroke>
	</s:Line>
	<s:Rect left="1" bottom="1" top="1" width="5">
		<s:fill>
			<s:LinearGradient>
				<s:entries>
					<s:GradientEntry color="0xF0F3F3" ratio="0" alpha="0.8"/>
					<s:GradientEntry color="0xFFFFFF" ratio="1" alpha="0.8"/>
				</s:entries>
			</s:LinearGradient>
		</s:fill>
	</s:Rect>
</s:Skin>
