.list > div.btn-panel{width:95%;margin:30px auto 0;}
/*.list > div:first-child input[type="button"]{margin-right:20px;}*/
.list #theList_filter{width:95%;margin:0 auto;color: #6C6C6C;}
.list #theList{width:95%;margin:10px auto 0;color: #6C6C6C;border-collapse: separate;border-bottom:1px solid #adadad;background-color:#FFF;}
.list #theList thead th{background:transparent url(../images/bg-form-title.png) 0 0 repeat-x;height:36px;text-align:center;border-top:1px solid #adadad;border-bottom:1px solid #adadad;}
.list #theList tbody tr td:first-child{border-left:1px solid #adadad;}
.list #theList tbody tr td:last-child{border-right:1px solid #adadad;}
.list #theList thead th:first-child{border-left:1px solid #adadad;-webkit-border-radius:5px 0 0 0;-moz-border-radius:5px 0 0 0;border-radius:5px 0 0 0;}
.list #theList thead th:last-child{border-right:1px solid #adadad;-webkit-border-radius:0 5px 0 0;-moz-border-radius:0 5px 0 0;border-radius:0 5px 0 0;}
.list #theList tbody td{text-align:center;vertical-align:top;padding:8px;}
.list #theList tbody td input[type="text"]{width:auto;}
.list #theList tbody td textarea{width:100%;overflow-y:auto;}
.list #theList tbody tr.even{background-color:#dbe3e9;}
.list #theList tbody tr:hover{background-color:#3993d4;color:#fff;}
.list .fg-toolbar {width:95%;margin:0 auto;color: #6C6C6C;margin-top:10px;position:relative;}
.list .fg-toolbar div#theList_info{margin-left:100px;}
.list .fg-toolbar div{display:inline-block;}
.list .fg-toolbar div:last-child{position:absolute;right:0;width:315px;text-align:right;}
.list #theList tbody td:last-child a{background:transparent url(../images/bg-small-button-gray.png) 0 0 repeat-x;color:#6C6C6C;height:22px;line-height:22px;-webkit-border-radius:3px;-moz-border-radius:3px;border-radius:3px;text-align:center;padding:0 10px;margin:0 3px;border:1px solid #adadad;}
.list #theList tbody td:last-child a:hover{background:transparent url(../images/bg-small-button-gray-hover.png) 0 0 repeat-x;border:1px solid #db8319;color:#b27326;}
.list #theList tbody td:last-child a:active{background:transparent url(../images/bg-small-button-gray-active.png) 0 0 repeat-x;border:1px solid #d57a1d;color:#965104;}
.list #theList tbody td:last-child{white-space:nowrap;}
.list2  #theList tbody td:last-child{white-space:normal;}
.list .create-plan .btn-add-task{background-color:#0076cc;-webkit-border-radius:5px;-moz-border-radius:5px;border-radius:5px;width:150px;height:40px;line-height:40px;font-size:18px;float:right;}
.list .create-plan .btn-add-task .add{background:transparent url(../images/icons.png) 0 -1550px no-repeat;width:16px;height:16px;display:inline-block;vertical-align:middle;margin:0 15px;}




