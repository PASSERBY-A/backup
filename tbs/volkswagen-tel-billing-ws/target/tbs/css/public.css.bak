body{font-size:12px;font-family:"微软雅黑";}
a{display:inline-block;}
a:link, a:active, a:visited{color:#fff;text-decoration:none;}
.banner{background:transparent url(../images/bg-banner.png) 0 0 repeat-x;height:90px;width:1300px;margin:0 auto;position:relative;}
.logo{width:132px;height:36px;background:transparent url(../images/logo-SMMS.png) 0 0 no-repeat;position:absolute;top:22px;left:15px;}
.banner .register{position:absolute;top:50px;right:30px;}
.btnpanel{border:1px solid #195da5;position:absolute;top:0;right:28px;margin:5px 0;background:transparent url(../images/bg-button-profile.png) 0 0 repeat-x;-webkit-border-radius:5px;-moz-border-radius:5px;border-radius:5px;}
.btnpanel a{width:70px;padding-right:8px;height:29px;line-height:29px;color:#94d1fd;text-align:right;}
.btnpanel a.profile{background:transparent url(../images/icons.png) 14px -943px no-repeat;border-right:1px solid #458cc6;}
.btnpanel a.logout{background:transparent url(../images/icons.png) 14px -990px no-repeat;border-left:1px solid #1c68ae;}

.clear{clear:both;}
.body{background-color:#444;width:1300px;margin:0 auto;min-height:500px;position:relative;}
.nav{width:219px;color:#9c9c9c;}
.nav span{display:inline-block;margin:0 30px 0 30px;}
.nav li a{width:219px;padding:12px 0;color:#9c9c9c;position:relative;}
.nav li a:hover{color:#3993d4;background:transparent url(../images/icons.png) 0 -850px no-repeat;}
.nav li a.selected{background:transparent url(../images/icons.png) 0 -900px no-repeat;color:#3993d4}
.nav .separator{border-top:1px solid #393939;border-bottom:1px solid #4e4e4e;height:0px;padding:0px;}
.nav .icon{width:22px;height:20px;margin-bottom:-8px;}
.nav .home{background:transparent url(../images/icons.png) 0 0 no-repeat;}
.nav li a:hover .home, .nav li a.selected .home{background:transparent url(../images/icons.png) 0 -50px no-repeat;}
.nav .sina{background:transparent url(../images/icons.png) 0 -100px no-repeat;}
.nav li a:hover .sina, .nav li a.selected .sina{background:transparent url(../images/icons.png) 0 -150px no-repeat;}
.nav .weixin{background:transparent url(../images/icons.png) 0 -200px no-repeat;}
.nav li a:hover .weixin, .nav li a.selected .weixin{background:transparent url(../images/icons.png) 0 -250px no-repeat;}
.nav .message{background:transparent url(../images/icons.png) 0 -300px no-repeat;}
.nav li a:hover .message, .nav li a.selected .message{background:transparent url(../images/icons.png) 0 -350px no-repeat;}
.nav .mail{background:transparent url(../images/icons.png) 0 -398px no-repeat;}
.nav li a:hover .mail, .nav li a.selected .mail{background:transparent url(../images/icons.png) 0 -448px no-repeat;}
.nav .analyse{background:transparent url(../images/icons.png) 0 -500px no-repeat;}
.nav li a:hover .analyse, .nav li a.selected .analyse{background:transparent url(../images/icons.png) 0 -550px no-repeat;}
.nav .plan{background:transparent url(../images/icons.png) 0 -600px no-repeat;}
.nav li a:hover .plan, .nav li a.selected .plan{background:transparent url(../images/icons.png) 0 -650px no-repeat;}
.nav .activity{background:transparent url(../images/icons.png) 0 -700px no-repeat;}
.nav li a:hover .activity, .nav li a.selected .activity{background:transparent url(../images/icons.png) 0 -750px no-repeat;}
.nav .facebook{background:transparent url(../images/icons.png) 0 -1950px no-repeat;}
.nav li a:hover .facebook, .nav li a.selected .facebook{background:transparent url(../images/icons.png) 0 -2000px no-repeat;}
.nav .count{background:transparent url(../images/icons.png) 0 -800px no-repeat;width:21px;height:20px;line-height:20px;text-align:center;position:absolute;top:11px;right:8px;color:#fff;}

.expansion{background:transparent url(../images/icons.png) 0 -1050px no-repeat;width:40px;height:77px;position:absolute;top:215px;left:210px;}
